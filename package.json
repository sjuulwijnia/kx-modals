{
	"name": "kx-modals",
	"version": "0.2.2",
	"description": "Angular 2 modals done right.",
	"main": "./index.js",
	"scripts": {
		"start": "npm run dev",
		"dev": "webpack-dev-server --config ./webpack.demo.js",
		"webpack": "webpack --config ./webpack.demo.js",
		"build": "npm run build:ngc && npm run build:root",
		"build:ngc": "ngc -p ./tsconfig.ngc.json",
		"build:root": "tsc -p ./tsconfig.root.json",
		"prebuild": "rimraf \"dist/**/*\"",
		"postbuild": "rimraf \"**/*.ngfactory.ts\" \"**/*.ngsummary.json\"",
		"ngc": "ngc --help"
	},
