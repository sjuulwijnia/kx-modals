{
	"compileOnSave": false,
	"compilerOptions": {
		"outDir": "./out-tsc-example",
		"sourceMap": true,
		"declaration": false,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"target": "es2016",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2017",
			"dom"
		],
		"baseUrl": ".",
		"rootDir": "./",
		"paths": {
			"kx-modals": [
				"./lib"
			]
		}
	},
	"exclude": [
		"node_modules",
		"**/*/node_modules"
	]
}
